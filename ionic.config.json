{
  "name": "Indicadores",
  "integrations": {
    "capacitor": {
      "plugins": {
        "CapacitorHttp": {
          "enabled": true
        }
      }
    }
  },
  "type": "angular",
  "budgets": [
    {
      "type": "initial",
      "maximumWarning": "5kb",
      "maximumError": "10kb"
    },
    {
      "type": "anyComponentStyle",
      "maximumWarning": "2kb",
      "maximumError": "4kb"
    }
  ]
}

